

State {
  Eq clickCount 0
  Eq random 'Hi'
}

Do {
  
  updateCount ( message ) [
    Jetlog message

    Var newVar
    Eq newVar 9
    Jetlog newVar

    Add clickCount 1
    Update clickCount
  ]

}

View {
  Div () [
    Button ( Click updateCount 'Im a param' ) [ Text 'Click me!' ]
    Span () [ Text clickCount ]
  ]
}

Jetlog
  message
Var
  newVar
Eq  
  newVar
  9
Jetlog
  newVar
Add
  clickCount
  1
Update
  clickCount